{
  "manifest_version":2,
  "name": "Wikit",
  "version": "1.0",
  "description": "Search Wikipedia on the go!",
  "icons": {
    "128":"icon128.svg",
    "48":"icon48.svg",
    "16":"icon16.svg"
  },

  "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  },

  "permissions": [
    "tabs",
    "contextMenus"
  ]
}
